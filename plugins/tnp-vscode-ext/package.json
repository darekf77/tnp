{
  "name": "tnp-vscode-ext",
  "displayName": "tnp-vscode-ext",
  "publisher": "morphi-io",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.30.0"
  },
  "repository": {
    "url": "https://github.com/darekf77/tsc-npm-project.git",
    "type": "git"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.tnpCLIcreateFiredevModel",
        "title": "FIREDEV: Create model"
      },
      {
        "command": "extension.tnpCLIBuildProject",
        "title": "FIREDEV: build project"
      },
      {
        "command": "extension.tnpCLIShowTempFiles",
        "title": "FIREDEV: show temp files"
      },
      {
        "command": "extension.tnpCLIHideTempFiles",
        "title": "FIREDEV: hide temp files"
      },
      {
        "command": "extension.tnpCLIRebuildExtension",
        "title": "FIREDEV: rebuild/reinstal vscode ext"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "extension.tnpCLIcreateFiredevModel"
        },
        {
          "command": "extension.tnpCLIBuildProject"
        },
        {
          "command": "extension.tnpCLIShowTempFiles"
        },
        {
          "command": "extension.tnpCLIHideTempFiles"
        },
        {
          "command": "extension.tnpCLIRebuildExtension"
        }
      ],
      "editor/title/context": [
        {
          "command": "extension.tnpCLIcreateFiredevModel",
          "group": "Firedev CLI essentials"
        },
        {
          "command": "extension.tnpCLIBuildProject",
          "group": "Firedev CLI essentials"
        },
        {
          "command": "extension.tnpCLIShowTempFiles",
          "group": "Firedev CLI essentials"
        },
        {
          "command": "extension.tnpCLIHideTempFiles",
          "group": "Firedev CLI essentials"
        },
        {
          "command": "extension.tnpCLIRebuildExtension",
          "group": "Firedev CLI essentials"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "build": "vsce package",
    "build:install": "npm run build && code --install-extension tnp-vscode-ext-0.0.1.vsix",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^3.1.4",
    "vscode": "^1.1.25",
    "tslint": "^5.8.0",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42"
  }
}
