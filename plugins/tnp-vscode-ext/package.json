{
  "name": "tnp-vscode-ext",
  "displayName": "tnp-vscode-ext",
  "publisher": "morphi-io",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.30.0"
  },
  "repository": {
    "url": "https://github.com/darekf77/tsc-npm-project.git",
    "type": "git"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.fIREDEVshowtempfiles",
        "title": "FIREDEV: show temp files"
      },
      {
        "command": "extension.fIREDEVhidetempfiles",
        "title": "FIREDEV: hide temp files"
      },
      {
        "command": "extension.fIREDEVbuilddist",
        "title": "FIREDEV: build dist"
      },
      {
        "command": "extension.fIREDEVrebuildreinstalvscodeext",
        "title": "FIREDEV: rebuild/reinstal vscode ext"
      },
      {
        "command": "extension.fIREDEVTESTEXT",
        "title": "FIREDEV: TEST EXT"
      },
      {
        "command": "extension.fIREDEVshowversion",
        "title": "FIREDEV: show version"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "extension.fIREDEVshowtempfiles"
        },
        {
          "command": "extension.fIREDEVhidetempfiles"
        },
        {
          "command": "extension.fIREDEVbuilddist"
        },
        {
          "command": "extension.fIREDEVrebuildreinstalvscodeext"
        },
        {
          "command": "extension.fIREDEVTESTEXT"
        },
        {
          "command": "extension.fIREDEVshowversion"
        }
      ],
      "editor/title/context": [
        {
          "command": "extension.fIREDEVshowtempfiles",
          "group": "Firedev CLI essentials"
        },
        {
          "command": "extension.fIREDEVhidetempfiles",
          "group": "Firedev CLI essentials"
        },
        {
          "command": "extension.fIREDEVbuilddist",
          "group": "Firedev CLI essentials"
        },
        {
          "command": "extension.fIREDEVrebuildreinstalvscodeext",
          "group": "Firedev CLI essentials"
        },
        {
          "command": "extension.fIREDEVTESTEXT",
          "group": "Firedev CLI essentials"
        },
        {
          "command": "extension.fIREDEVshowversion",
          "group": "Firedev CLI essentials"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "build": "vsce package",
    "build:install": "ts-node update-proj.js && npm run build && code --install-extension tnp-vscode-ext-0.0.1.vsix",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^3.1.4",
    "vscode": "^1.1.25",
    "tslint": "^5.8.0",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42",
    "ts-node": "^7.0.1",
    "lodash": "^4.17.4"
  }
}