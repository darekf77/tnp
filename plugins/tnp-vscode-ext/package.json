        "command": "extension.firedevhidetempfiles",
        "title": "FIREDEV: hide temp files"
      },
      {
        "command": "extension.firedevbuilddist",
        "title": "FIREDEV: build dist"
      },
      {
        "command": "extension.firedevstaticbuilddist",
        "title": "FIREDEV: static build dist"
      },
      {
        "command": "extension.firedevresetproject",
        "title": "FIREDEV: reset project"
      },
      {
        "command": "extension.firedevresetallprojects",
        "title": "FIREDEV: reset all projects"
      },
      {
        "command": "extension.firedevclearproject",
        "title": "FIREDEV: clear project"
      },
      {
        "command": "extension.firedevclearallprojects",
        "title": "FIREDEV: clear all projects"
      },
      {
        "command": "extension.firedevquickgitcommitandpushupdate",
