{
  "html.format.wrapAttributes": "force-aligned",
  "editor.rulers": [
    80,
    100,
    120
  ],
  "tslint.enable": false,
  "workbench.colorCustomizations": {
    "activityBar.background": "#bcdd70",
    "activityBar.foreground": "#ffffff",
    "statusBar.background": "#bcdd70"
  },
  "workbench.colorTheme": "Default Light+",
  "files.exclude": {
    "node_modules": true,
    "tnp-system-path.txt": true,
    "tmp*": true,
    "dist*": true,
    "bundle*": true,
    "browser": true,
    "browser*": true,
    "module*": true,
    "backup": true,
    "module": true,
    "www": true,
    "Thumbs.db": true,
    ".DS_Store": true,
    "**/.DS_Store": true,
    "npm-debug.log*": true,
    ".sass-cache": true,
    ".sourcemaps": true,
    "environment.js": true,
    "environment.dev.js": true,
    "environment.prod.js": true,
    "environment.test.js": true,
    "environment.stage.js": true,
    "environment.static.js": true,
    "environment.online.js": true,
    "__environment.js": true,
    "__environment.dev.js": true,
    "__environment.prod.js": true,
    "__environment.test.js": true,
    "__environment.stage.js": true,
    "__environment.static.js": true,
    "__environment.online.js": true,
    "!custom/environment.js": true,
    "!custom/environment.dev.js": true,
    "!custom/environment.prod.js": true,
    "!custom/environment.test.js": true,
    "!custom/environment.stage.js": true,
    "!custom/environment.static.js": true,
    "!custom/environment.online.js": true,
    "tslint.json": true,
    ".editorconfig": true,
    "environment.d.ts": true,
    ".gitignore": true,
    ".npmignore": true,
    ".npmrc": true,
    ".babelrc": true
  }
}