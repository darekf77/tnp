{
  "html.format.wrapAttributes": "force-aligned",
  "editor.rulers": [
    80,
    100,
    120
  ],
  "tslint.enable": false,
  "workbench.colorCustomizations": {
    "activityBar.background": "#b8654c",
    "activityBar.foreground": "#000000",
    "statusBar.background": "#bcdd70",
    "statusBar.debuggingBackground": "#bcdd70"
  },
  "workbench.colorTheme": "Default Light+",
  "files.exclude": {
    "node_modules": true,
    "tnp-system-path.txt": true,
    "tmp*": true,
    "dist*": true,
    "bundle*": true,
    "browser": true,
    "browser*": true,
    "module*": true,
    "backup": true,
    "module": true,
    "www": true,
    "Thumbs.db": true,
    ".DS_Store": true,
    "**/.DS_Store": true,
    "npm-debug.log*": true,
    ".sass-cache": true,
    ".sourcemaps": true,
    "tsconfig.isomorphic.json": true,
    "tsconfig.json": true,
    "src/tsconfig.app.json": true,
    ".angular-cli.json": true,
    "src": true,
    "components": true,
    "__src": true,
    "__components": true,
    "!custom/src": true,
    "!custom/components": true,
    "tslint.json": true,
    ".editorconfig": true,
    "index.js": true,
    "index.d.ts": true,
    "index.js.map": true,
    "tsconfig.browser.json": true,
    "karma.conf.js.filetemplate": true,
    "tsconfig.isomorphic.json.filetemplate": true,
    "tsconfig.json.filetemplate": true,
    "src/tsconfig.app.json.filetemplate": true,
    ".angular-cli.json.filetemplate": true,
    "src/tsconfig.packages.json": true,
    "src/typings.d.ts": true,
    "src/main.ts": true,
    "src/polyfills.ts": true,
    "src/tsconfig.spec.json": true,
    "protractor.conf.js": true,
    "karma.conf.js": true,
    ".gitignore": true,
    ".npmignore": true,
    ".npmrc": true,
    ".babelrc": true
  }
}
