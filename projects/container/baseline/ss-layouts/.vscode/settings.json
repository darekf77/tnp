{
  "html.format.wrapAttributes": "force-aligned",
  "editor.rulers": [
    80,
    100,
    120
  ],
  "tslint.enable": false,
  "workbench.colorCustomizations": {
    "activityBar.background": "#4cb8a6",
    "activityBar.foreground": "#000000",
    "statusBar.background": "#c98e72",
    "statusBar.debuggingBackground": "#c98e72"
  },
  "workbench.colorTheme": "Default Light+",
  "files.exclude": {
    "node_modules": true,
    "tnp-system-path.txt": true,
    "tmp*": true,
    "dist*": true,
    "bundle*": true,
    "browser": true,
    "browser*": true,
    "module*": true,
    "backup": true,
    "module": true,
    "www": true,
    "Thumbs.db": true,
    ".DS_Store": true,
    "**/.DS_Store": true,
    "npm-debug.log*": true,
    ".sass-cache": true,
    ".sourcemaps": true,
    "src/tsconfig.app.json": true,
    "tslint.json": true,
    ".editorconfig": true,
    "index.js": true,
    "index.d.ts": true,
    "index.js.map": true,
    "ng-package.json": true,
    "tsconfig-aot.bundle.json": true,
    "tsconfig-aot.dist.json": true,
    "tsconfig.isomorphic.json": true,
    "tsconfig.browser.json": true,
    "src/tsconfig.app.json.filetemplate": true,
    "src/tsconfig.packages.json": true,
    "tsconfig.json": true,
    "src/typings.d.ts": true,
    "src/main.ts": true,
    "src/polyfills.ts": true,
    "src/tsconfig.spec.json": true,
    "protractor.conf.js": true,
    "karma.conf.js": true,
    ".gitignore": true,
    ".npmignore": true,
    ".npmrc": true,
    ".babelrc": true
  }
}