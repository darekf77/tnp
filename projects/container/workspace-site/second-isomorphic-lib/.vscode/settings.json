{
  "files.exclude": {
    "node_modules": true,
    "tnp-system-path.txt": true,
    "tmp*": true,
    "dist*": true,
    "bundle*": true,
    "browser": true,
    "browser*": true,
    "module*": true,
    "backup": true,
    "module": true,
    "www": true,
    "Thumbs.db": true,
    ".DS_Store": true,
    "**/.DS_Store": true,
    "npm-debug.log*": true,
    ".sass-cache": true,
    ".sourcemaps": true,
    "tslint.json": true,
    ".editorconfig": true,
    "index.js": true,
    "index.d.ts": true,
    "index.js.map": true,
    ".vscode/launch.json": true,
    "tsconfig.json": true,
    "tsconfig.browser.json": true,
    "webpack.config.js": true,
    "run.js": true,
    "src/typings.d.ts": true,
    "src/entities.ts": true,
    "src/controllers.ts": true,
    ".gitignore": true,
    ".npmignore": true,
    ".npmrc": true,
    ".babelrc": true
  },
  "workbench.colorCustomizations": {
    "activityBar.background": "#0084ff",
    "activityBar.foreground": "#000000"
  },
  "editor.rulers": [
    100,
    120
  ],
  "html.format.wrapAttributes": "force-aligned",
  "workbench.colorTheme": "Default Light+",
  "tslint.autoFixOnSave": true,
  "eslint.enable": false,
  "tslint.enable": false
}