{
  "name": "angular-lib",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:app": "cpr bundle src/components && ng build --app packages && rimraf src/@my",
    "build:lib": "npm-run ng-packagr",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:esm": "npm-run gulp inline-templates && npm run ngcompile",
    "build:esm:watch": "npm-run gulp build:esm:watch",
    "ngcompile": "npm-run ngc -p tsconfig-aot.json",
    "clean:tmp": "rimraf coverage dist tmp"
  },
  "tnp": {
    "type": "angular-lib",
    "resources": [
      "README.md"
    ]
  },
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/cdk": "^5.0.4",
    "@angular/common": "~5.1.0",
    "@angular/compiler": "~5.1.0",
    "@angular/core": "~5.1.0",
    "@angular/forms": "~5.1.0",
    "@angular/http": "~5.1.0",
    "@angular/material": "^5.0.4",
    "@angular/platform-browser": "~5.1.0",
    "@angular/platform-browser-dynamic": "~5.1.0",
    "@angular/router": "~5.1.0",
    "core-js": "^2.4.1",
    "hammerjs": "^2.0.8",
    "rxjs": "^5.4.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular-devkit/schematics": "^0.0.42",
    "@angular/cli": "~1.6.0",
    "@angular/compiler-cli": "~5.1.0",
    "@angular/language-service": "~5.1.0",
    "@types/jasmine": "~2.8.0",
    "@types/node": "~8.5.0",
    "codelyzer": "~4.0.0",
    "cpr": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-inline-ng2-template": "^4.1.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.0",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "2.0.0-rc.9",
    "protractor": "~5.2.0",
    "rimraf": "^2.6.1",
    "ts-node": "~4.1.0",
    "tsickle": "^0.26.0",
    "tslint": "~5.8.0",
    "typescript": "~2.5.3"
  }
