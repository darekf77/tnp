#!/usr/bin/env node
//#region @backend
var fs = require('fs')
var path = require('path')
var pathToDistRun = path.join(__dirname, '../dist/start.js');
var pathToBundletRun = path.join(__dirname, '../start.js');

if (fs.existsSync(pathToDistRun)) {
  var start = require(pathToDistRun.replace(/\.js$/g, '')).start;
} else {
  var start = require(pathToBundletRun.replace(/\.js$/g, '')).start;
}
start(process.argv);
//#endregion
