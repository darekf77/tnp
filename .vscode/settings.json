{
  "files.exclude": {
    "/node_modules": true,
    "/tmp*": true,
    "/dist*": true,
    "/bundle*": true,
    "/browser": true,
    "/backup": true,
    "/module": true,
    "/www": true,
    "/Thumbs.db": true,
    "/.DS_Store": true,
    "/**/.DS_Store": true,
    "/npm-debug.log*": true,
    "/.sass-cache": true,
    "/.sourcemaps": true,
    "/tslint.json": true,
    "/.editorconfig": true,
    "/index.js": true,
    "/index.d.ts": true,
    "/index.js.map": true,
    "/.vscode/launch.json": true,
    "/run.js": true,
    "/src/typings.d.ts": true,
    "/tsconfig.json": true,
    "/tsconfig.browser.json": true,
    ".gitignore": true,
    ".npmignore": true,
    ".npmrc": true,
    ".babelrc": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/projects": true,
    "projects": true
  },
  "workbench.colorCustomizations": {
    "activityBar.background": "#5c3735"
  },
  "editor.rulers": [
    100,
    120
  ],
  "html.format.wrapAttributes": "force-aligned",
  "workbench.colorTheme": "Default Light+",
  "background.enabled": true,
  "background.useDefault": false,
  "background.styles": [
    {
      "opacity": 0.05
    }
  ],
  "background.customImages": [
    "https://avatars1.githubusercontent.com/u/36506552?s=400&u=380c1c398a14720b9f140f8e526b1e052c9e9699&v=4"
  ]
}
