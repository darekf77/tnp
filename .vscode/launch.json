{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Attach to global cli tool",
      "port": 9229,
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Server tnp for tnp",
      "program": "${workspaceFolder}/run.js",
      "args": [
        "--ENVoverride=%7B%0A%20%20%20%20%22clientProjectName%22%3A%20%22tnp%22%0A%7D"
      ],
      "runtimeArgs": [
        "--experimental-worker"
      ]
    }
  ],
  "compounds": []
}